{
  "name": "probillify",
  "version": "1.0.0",
  "description": "Make invoices with just a few clicks!",
  "main": "dist/main.js",
  "scripts": {
    "tree": "tree -I 'node_modules' > filetree.txt",
    "start": "electron-forge start",
    "build": "tsc",
    "test": "vitest",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:dmg": "electron-forge make --targets=@electron-forge/maker-dmg",
    "make:zip": "electron-forge make --targets=@electron-forge/maker-zip",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "electron:dmg": "electron-installer-dmg ./out/probillify-darwin-arm64/probillify.app Probillify --overwrite"
  },
  "keywords": [],
  "author": "Luke Arthur",
  "license": "ISC",
